{
  "private": true,
  "name": "@home-inventory/client",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"next dev\" \"npm run build:typed-css:watch\"",
    "build": "npm run build:typed-css && next build",
    "build:typed-css": "yarn typed-scss-modules src --ignore \"**/table.scss\" --ignore \"**/nprogress.scss\" --ignore \"**/index.scss\"",
    "build:typed-css:watch": "yarn typed-scss-modules src -w --ignore \"**/table.scss\" --ignore \"**/nprogress.scss\" --ignore \"**/index.scss\"",
    "start": "next start",
    "prod": "npm run start",
    "test-tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@casper124578/useful": "^1.5.6",
    "@react-aria/ssr": "^3.1.2",
    "axios": "^0.26.1",
    "cookie": "^0.4.2",
    "date-fns": "^2.28.0",
    "next": "^12.1.4",
    "nprogress": "^0.2.0",
    "preact": "^10.10.0",
    "react": "17.0.2",
    "react-cool-onclickoutside": "^1.7.0",
    "react-dom": "17.0.2",
    "react-hot-toast": "^2.2.0",
    "react-redux": "^7.2.8",
    "react-select": "^5.2.2",
    "react-tooltip": "^4.2.21",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "sass": "^1.49.10"
  },
  "devDependencies": {
    "@types/cookie": "^0.4.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.43",
    "@types/react-dom": "^17.0.14",
    "concurrently": "^7.0.0",
    "typed-scss-modules": "^6.2.0",
    "typescript": "4.6.3"
  }
}
