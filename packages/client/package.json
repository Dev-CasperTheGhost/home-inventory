{
  "private": true,
  "name": "@home-inventory/client",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"next dev\" \"npm run build:typed-css:watch\"",
    "build": "npm run build:typed-css && next build",
    "build:typed-css": "tsm src --ignore \"**/table.scss\" --ignore \"**/nprogress.scss\" --ignore \"**/index.scss\"",
    "build:typed-css:watch": "tsm src -w --ignore \"**/table.scss\" --ignore \"**/nprogress.scss\" --ignore \"**/index.scss\"",
    "start": "next start",
    "prod": "npm run start",
    "test-tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@casper124578/useful": "^1.4.4",
    "axios": "^0.24.0",
    "cookie": "^0.4.1",
    "date-fns": "^2.28.0",
    "next": "^12.0.7",
    "nprogress": "^0.2.0",
    "preact": "^10.5.15",
    "react": "17.0.2",
    "react-cool-onclickoutside": "^1.7.0",
    "react-dom": "17.0.2",
    "react-hot-toast": "^2.1.1",
    "react-redux": "^7.2.6",
    "react-select": "^5.2.1",
    "react-tooltip": "^4.2.21",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "sass": "^1.45.2"
  },
  "devDependencies": {
    "@types/cookie": "^0.4.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.38",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^6.5.1",
    "typed-scss-modules": "^4.1.1",
    "typescript": "4.6.2"
  }
}
